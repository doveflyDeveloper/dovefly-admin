<?xml version="1.0" encoding="UTF-8"?>

<workbook> 
  <worksheet name="学校区划"> 
    <section startRow="0" endRow="0"/>  
    <loop startRow="1" endRow="1" items="beans" var="bean" varType="com.deertt.module.region.region.vo.RegionVo"> 
      <section startRow="1" endRow="1"> 
        <mapping row="1" col="0">bean.code</mapping>
        <mapping row="1" col="1">bean.name</mapping>
        <mapping row="1" col="2">bean.full_name</mapping>
        <mapping row="1" col="3">bean.parent_id</mapping>
        <mapping row="1" col="4">bean.parent_code</mapping>
        <mapping row="1" col="5">bean.parent_name</mapping>
        <mapping row="1" col="6">bean.parent_full_name</mapping>
        <mapping row="1" col="7">bean.level</mapping>
        <mapping row="1" col="8">bean.is_leaf</mapping>
        <mapping row="1" col="9">bean.remark</mapping>
      </section>  
      <loopbreakcondition> 
        <rowcheck offset="0"> 
          <cellcheck offset="0"/> 
        </rowcheck> 
      </loopbreakcondition> 
    </loop> 
  </worksheet> 
</workbook>
