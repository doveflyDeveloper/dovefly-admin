<?xml version="1.0" encoding="UTF-8"?>

<workbook> 
  <worksheet name="城市"> 
    <section startRow="0" endRow="0"/>  
    <loop startRow="1" endRow="1" items="beans" var="bean" varType="com.deertt.module.sys.city.vo.CityVo"> 
      <section startRow="1" endRow="1"> 
        <mapping row="1" col="0">bean.name</mapping>
        <mapping row="1" col="1">bean.remark</mapping>
      </section>  
      <loopbreakcondition> 
        <rowcheck offset="0"> 
          <cellcheck offset="0"/> 
        </rowcheck> 
      </loopbreakcondition> 
    </loop> 
  </worksheet> 
</workbook>
