<?xml version="1.0" encoding="UTF-8"?>

<workbook> 
  <worksheet name="盘点明细"> 
    <section startRow="0" endRow="0"/>  
    <loop startRow="1" endRow="1" items="beans" var="bean" varType="com.deertt.module.mm.inventory.vo.InventoryDetailVo"> 
      <section startRow="1" endRow="1"> 
        <mapping row="1" col="0">bean.goods_id</mapping>
        <mapping row="1" col="1">bean.goods_name</mapping>
        <mapping row="1" col="2">bean.check_quantity</mapping>
      </section>  
      <loopbreakcondition> 
        <rowcheck offset="0"> 
          <cellcheck offset="0"/> 
        </rowcheck> 
      </loopbreakcondition> 
    </loop> 
  </worksheet> 
</workbook>
